    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "rollup": "^3.12.0",
    "rollup-plugin-node-externals": "^5.1.0",
    "terser": "^5.16.1",
    "typescript": "^4.9.4",
    "verdaccio": "^5.20.1",
    "verdaccio-htpasswd": "^11.0.0-6-next.13"
  },
  "peerDependencies": {
    "verdaccio": "^5||>=6.0.0-6-next.0"
  },
  "engines": {
    "node": "^14||^16||^18"
