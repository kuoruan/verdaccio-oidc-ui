    "@verdaccio/url": "^11.0.0-6-next.21",
    "chalk": "^5.2.0",
    "core-js": "^3.27.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "global-agent": "^3.0.0",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
