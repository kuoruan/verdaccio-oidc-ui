{
  "$schema": "http://json.schemastore.org/package",
  "name": "verdaccio-oidc-ui",
  "version": "3.3.0",
  "repository": "https://github.com/kuoruan/verdaccio-oidc-ui",
  "license": "MIT",
  "author": {
    "name": "Xingwang Liao",
    "email": "kuoruan@gmail.com"
  },
  "description": "A UI for OIDC authentication for Verdaccio, a fork of verdaccio-github-oauth-ui",
  "main": "dist/server/index.js",
  "module": "dist/server/index.mjs",
  "bin": {
    "verdaccio-oidc-ui": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "start": "verdaccio -c verdaccio.yaml",
    "lint": "eslint --fix --ext .js,.ts .",
    "link:oidc": "yarn link && yarn link verdaccio-oidc-ui",
    "unlink:oidc": "yarn unlink verdaccio-oidc-ui && yarn unlink",
    "prepublishOnly": "yarn run build"
  },
  "dependencies": {
    "@gitbeaker/node": "^35.8.0",
    "@isaacs/ttlcache": "^1.2.1",
    "@verdaccio/auth": "^6.0.0-6-next.34",
