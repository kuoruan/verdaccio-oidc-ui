{
  "$schema": "http://json.schemastore.org/package",
  "name": "verdaccio-openid",
  "version": "0.6.1",
  "repository": "https://github.com/kuoruan/verdaccio-openid",
  "license": "MIT",
  "author": {
    "name": "Xingwang Liao",
    "email": "kuoruan@gmail.com"
  },
  "description": "A UI for OIDC authentication for Verdaccio, a fork of verdaccio-github-oauth-ui",
  "keywords": [
    "verdaccio",
    "oidc",
    "openid",
    "authentication",
    "oauth",
    "oauth2",
    "ui"
  ],
  "main": "./dist/server/index.js",
  "module": "./dist/server/index.mjs",
  "bin": "./dist/verdaccio-openid.mjs",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "packageManager": "pnpm@8.6.5",
  "scripts": {
    "build": "rimraf dist && rollup -c --environment NODE_ENV:production",
    "start": "cross-env DEBUG='verdaccio:*' verdaccio -c verdaccio/verdaccio.yml",
    "preview": "verdaccio -c verdaccio/verdaccio.yml",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts --fix .",
    "link:openid": "pnpm link --global verdaccio-openid",
    "unlink:openid": "pnpm rm --global verdaccio-openid",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@gitbeaker/node": "^35.8.1",
    "@isaacs/ttlcache": "^1.4.0",
    "@verdaccio/auth": "6.0.0-6-next.51",
    "@verdaccio/config": "6.0.0-6-next.72",
    "@verdaccio/core": "6.0.0-6-next.72",
    "@verdaccio/signature": "^6.0.0-6-next.2",
    "@verdaccio/url": "11.0.0-6-next.38",
    "core-js": "^3.31.0",
    "debug": "^4.3.4",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "global-agent": "^3.0.0",
    "minimist": "^1.2.8",
    "open": "^9.1.0",
    "openid-client": "^5.4.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/express": "^4.17.17",
    "@types/global-agent": "^2.1.1",
    "@types/lodash": "^4.14.195",
    "@types/minimist": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@verdaccio/types": "^10.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.0",
    "rollup-plugin-node-externals": "^6.1.1",
    "rollup-plugin-shebang-bin": "^0.0.5",
    "typescript": "^5.1.6",
    "verdaccio": "^5.25.0",
    "verdaccio-htpasswd": "^11.0.0-6-next.13"
  },
  "peerDependencies": {
    "verdaccio": ">=5.0.0||>=6.0.0-6-next.0"
  },
  "engines": {
    "node": ">=16"
  }
}
